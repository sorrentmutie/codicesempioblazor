@typeparam TItem

@if(Items != null)
{
    @foreach(var item in Items)
    {
        if(ChildContent != null)
        {
            @ChildContent(item)           
        }
    }
}

@code {
    [CascadingParameter] public IEnumerable<TItem>? Items { get; set; }
    [Parameter] public RenderFragment<TItem>? ChildContent { get; set; }
}
